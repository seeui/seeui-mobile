@require '../css/base'

@font-face
    font-family: "seeui-mobile-icon"
    src:
        url('./font/icon.woff?t=1499157214944') format('woff'), /* chrome, firefox */
        url('./font/icon.ttf?t=1499157214944') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
        url('./font/icon.svg?t=1499157214944#seeui-mobile-icon') format('svg') /* iOS 4.1- */

.cui-icon
    font-family:"seeui-mobile-icon" !important
    font-size:16px
    font-style:normal
    display: inline-block
    -webkit-font-smoothing: antialiased
    -moz-osx-font-smoothing: grayscale

